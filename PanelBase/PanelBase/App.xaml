             xmlns:local="clr-namespace:PanelBase"
             StartupUri="MainWindow.xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="DictionaryComboBox.xaml"></ResourceDictionary>
                <ResourceDictionary Source="DictionaryButton.xaml"></ResourceDictionary>
                <ResourceDictionary Source="DictionaryListView.xaml"></ResourceDictionary>
                <ResourceDictionary Source="DictionaryTextBox.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
